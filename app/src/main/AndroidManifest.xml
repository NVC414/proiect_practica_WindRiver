<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyApplication"
        >
        <activity
            android:name=".ui.LoginRegister.Register_activity"
            android:theme="@style/Theme.Material3.Light.NoActionBar"
            android:exported="false" />
        <activity
            android:name=".ui.LoginRegister.Login_activity"
            android:theme="@style/Theme.Material3.Light.NoActionBar"

            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            >

        </activity>
        <activity
            android:name=".ui.DetailView.CaseDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.DetailView.CpuDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.DetailView.MemoryDetailsActivity"
            android:exported="false" />

        <activity android:name=".ui.DetailView.MotherboardDetailsActivity"
            android:exported="false" />
        <activity android:name=".activity.GpuDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".ui.DetailView.PsuDetailsActivity"
            android:exported="false" />
    </application>

</manifest>